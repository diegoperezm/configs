# $XONSH_STYLE_OVERRIDES["Token.Comment.Multiline"]   = "#ffff00"
# $XONSH_STYLE_OVERRIDES["Token.Comment.Hashbang"]    = "#ffff00"
# $XONSH_STYLE_OVERRIDES["Token.Comment.PreprocFile"] = "#ffff00"
