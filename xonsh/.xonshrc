# XONSH WEBCONFIG START
# $PROMPT = '{env_name}{BOLD_GREEN}{user}@{hostname}{BOLD_BLUE} {cwd}{branch_color}{curr_branch: {}}{RESET} {BOLD_BLUE}{prompt_end}{RESET} '
# $PROMPT = '{env_name}'
# XONSH WEBCONFIG END
$PROMPT = '{BOLD_RED}[{RESET}{env_name}{BOLD_RED}][{RESET}{BOLD_YELLOW}{cwd}{BOLD_RED}][{RESET}{branch_color}{curr_branch:{}}{BOLD_RED}]\nâ””>{RESET} '
# $XONSH_COLOR_STYLE = 'solarized-dark'
xontrib load argcomplete jedi kitty readable-traceback
$AUTO_SUGGEST_IN_COMPLETIONS=True
$READABLE_TRACE_STRIP_PATH_ENV=True
$READABLE_TRACE_REVERSE=True
$READABLE_TRACE_ON_TTY=True
$PRETTY_PRINT_RESULTS
$XONSH_AUTOPAIR=True
# $COLOR_INPUT=False
$XONSH_STYLE_OVERRIDES["Token.Comment"]             = "#ffff00"
$XONSH_STYLE_OVERRIDES["Token.Literal.String"]      = "#ffff00"
$XONSH_STYLE_OVERRIDES["Token.Literal.Number"]      = "#ff7f00"
$XONSH_STYLE_OVERRIDES["Token.Operator"]            = "#ffffff"

